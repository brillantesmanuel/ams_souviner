{
  "name": "owel/ams-theme",
  "description": "Custom WordPress theme with Composer dependencies",
  "type": "wordpress-theme",
  "require": {
    "php": ">=8.0",
    "twbs/bootstrap": "^5.3"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7",
    "wp-coding-standards/wpcs": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "MyTheme\\": "src/"
    }
  },
  "config": {
    "vendor-dir": "vendor",
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
